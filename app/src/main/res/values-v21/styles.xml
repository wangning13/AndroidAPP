<resources>
    <style name="AppTheme" parent="@style/AppBaseTheme">

        <!-- 底部导航栏颜色 -->
        <item name="android:navigationBarColor">@color/colorPrimary</item>
    </style>

    <style name="AppTheme.NoActionBar">
